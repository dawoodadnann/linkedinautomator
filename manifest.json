{
  "manifest_version": 3,
  "name": "Google Search Scraper",
  "version": "1.0",
  "description": "Extracts Google search results and downloads as CSV",
  "permissions": ["activeTab", "scripting", "downloads"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search*"],
      "js": ["content.js"]
    }
  ]
}
